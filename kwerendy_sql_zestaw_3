CREATE TABLE zukal.kreatura SELECT * FROM wikingowie.kreatura;
CREATE TABLE zukal.zasob SELECT * FROM wikingowie.zasob;
CREATE TABLE zukal.ekwipunek SELECT * FROM wikingowie.ekwipunek;

zad 1.2

select * from zasob;

1.3

select * from zasob where rodzaj='jedzenie';

1.4 select idZasobu and ilosc from zasob where id in(1,3,5);

select idZasobu, ilosc from ekwipunek where idKreatury=1 or 3 or plplp

2.1
select * from kreatura where rodzaj!='wiedzma' and udzwig>50;

2.2
select * from zasob where waga>=2 and waga <=5;

zad 2.3
select * from kreatura where nazwa like '%or%' and udzwig>=30 and udzwig<=70;

zad 3.1
select * from zasob where month(dataPozyskania)='7' or month(dataPozyskania)='8';

zad 3.2
select * from zasob where rodzaj is not null order by waga asc;

zad 3.3
select * from kreatura order by data_ur asc limit 5;

zad 4.1
select rodzaj from zasob group by rodzaj having count(rodzaj)=1; ||
select distinct(rodzaj) from zasob;
